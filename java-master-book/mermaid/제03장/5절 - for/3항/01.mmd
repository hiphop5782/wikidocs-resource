graph TD
    aa[프로그램시작]
    aa --> a[cnt 초기화]
    a --> b[반복시작]
    b --> c[i = 1]
    c --> d[i가 100이하?]
    d --> |true| e[i가 짝수?]
    e --> |true| f[cnt 증가]
    d ----> |false| g[반복 종료]
    g --> h[cnt 출력]
    e --> |false| i
    f --> i[i 증가]
    i --> d
    h --> hh[프로그램종료]

style a fill:dodgerblue,color:white
style f fill:dodgerblue,color:white
style h fill:dodgerblue,color:white
